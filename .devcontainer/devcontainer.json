{
    "name": "Bisaplis Codespace",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "web",
    "workspaceFolder": "/app",
    "features": {
        "ghcr.io/devcontainers/features/python:1": {}
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "batisteo.vscode-django"
            ]
        }
    },
    "forwardPorts": [
        8000
    ]
}